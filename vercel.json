{
  "version": 2,
  "builds": [
    {
      "src": "Bproject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"  // Specify your Python version here
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/media/(.*)", "dest": "/media/$1" },
    { "src": "/(.*)", "dest": "/manage.py" },
    { "dest": "Bproject/wsgi.py"}
  ]
}
