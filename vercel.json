{
  "version": 2,
  "builds": [
    {
      "src": "Bproject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"  // Specify your Python version here
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "src": "/media/(.*)",
      "src": "/(.*)",
      "dest": "Bproject/wsgi.py"
    }
  ]
}
